    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "cli",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["-c", "${workspaceFolder}/conf/config_mean_solo.toml", "-v"],
             "preLaunchTask": "hatch-build",
        },

        {
            "name": "cli_biosphere",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["-c", "${workspaceFolder}/conf/config_svd_biosphere.toml", "-v"],
             "preLaunchTask": "hatch-build",
        },

        
        {
            "name": "provision",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["-c", "${workspaceFolder}/configs/config_provision_biosphere.toml", "-p", "scripts/provision_biosphere.sh"]
        }
    ]
}